<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="140.0" prefWidth="280.0" style="-fx-background-color: #000; -fx-border-color: #fff;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ConfirmController">
   <top>
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="57.0" prefWidth="242.0" text="Do you want to delete this node?" textAlignment="CENTER" textFill="WHITE" BorderPane.alignment="CENTER">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
   </top>
   <center>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <JFXButton fx:id="deleteButton" alignment="CENTER" layoutX="50.0" layoutY="14.0" prefHeight="30.0" prefWidth="80.0" style="-fx-background-color: #1aa3ff; -fx-background-radius: 0;" text="Delete">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="keepButton" alignment="CENTER" layoutX="149.0" layoutY="14.0" prefHeight="30.0" prefWidth="80.0" style="-fx-background-color: #a1a1a1; -fx-background-radius: 0;" text="Keep">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </JFXButton>
         </children>
      </AnchorPane>
   </center>
</BorderPane>
